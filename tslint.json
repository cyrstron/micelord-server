{  
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "interface-name": [false],
    "variable-name": [true, "allow-leading-underscore"],
    "no-string-literal": [false],
    "no-console": {
      "severity": "warning",
      "options": "log"
    }
  }
}